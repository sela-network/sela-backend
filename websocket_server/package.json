{
  "name": "sela-websocket-server",
  "version": "1.0.0",
  "description": "WebSocket server for real-time communication with Flutter apps",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "build:hybrid": "tsc && echo 'Hybrid server build completed'",
    "start": "node dist/server.js",
    "start:hybrid": "node dist/hybrid_server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "dev:hybrid": "nodemon",
    "debug": "ts-node --inspect-brk src/server.ts",
    "debug:hybrid": "ts-node --inspect-brk src/hybrid_server.ts",
    "debug:start": "ts-node --inspect src/server.ts",
    "debug:hybrid:start": "ts-node --inspect src/hybrid_server.ts",
    "test": "ts-node src/test/test-client.ts",
    "test:multi": "ts-node src/test/test-client.ts multi",
    "test:db": "ts-node src/test/database-test.ts",
    "monitor": "ts-node src/tools/monitor.ts",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "migrate-multijob": "ts-node src/scripts/migrate_to_multijob.ts",
    "cleanup-migration": "ts-node src/scripts/migrate_to_multijob.ts --cleanup",
    "migrate-referral-codes": "ts-node src/scripts/migrate_referral_codes.ts",
    "fix:check-victims": "ts-node src/scripts/fix_race_condition_victims.ts",
    "fix:compensate-victims": "ts-node src/scripts/fix_race_condition_victims.ts --apply",
    "docker:build": "docker build -t sela-websocket-server .",
    "docker:run": "docker run -p 8082:8082 sela-websocket-server"
  },
  "keywords": [
    "websocket",
    "sela",
    "flutter",
    "cbor",
    "realtime",
    "nodejs",
    "real-time-communication"
  ],
  "author": {
    "name": "Sela Network",
    "email": "contact@sela.network"
  },
  "license": "MIT",
  "dependencies": {
    "@dfinity/agent": "^3.2.4",
    "@dfinity/candid": "^3.2.4",
    "@dfinity/identity": "^3.2.4",
    "@dfinity/identity-secp256k1": "^3.2.4",
    "@dfinity/principal": "^3.2.4",
    "@types/node-cron": "^3.0.11",
    "cbor": "^9.0.1",
    "dotenv": "^17.2.1",
    "ed25519": "^0.0.5",
    "express": "^4.21.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "node-forge": "^1.3.1",
    "redis": "^4.6.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cbor": "^6.0.0",
    "@types/ed25519": "^0.0.3",
    "@types/express": "^4.17.23",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.55.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sela-network/websocket-server.git"
  },
  "bugs": {
    "url": "https://github.com/sela-network/websocket-server/issues"
  },
  "homepage": "https://github.com/sela-network/websocket-server#readme",
  "config": {
    "port": 8082,
    "host": "localhost"
  }
}
